{
  "name": "llmr-label-debug",
  "description": "Sync test to aid debugging label performance",
  "version": "0.0.0",
  "main": "js/test.js",
  "dependencies": {
    "geojson-rewind": "~0.1.0",
    "rbush": "~1.3.2",
    "pbf": "0.0.1",
    "unitbezier": "0.0.0"
  },
  "devDependencies": {
    "browserify": "~3.46.0",
    "glify": "^0.2.0",
    "tape": "~2.12.1",
    "watchify": "~0.8.3",
    "jshint": "^2.5.0",
    "hintify": "^0.1.0"
  },
  "browserify": {
    "transform": [
      "hintify",
      "glify"
    ]
  },
  "scripts": {
    "watch": "watchify -v -d js/test.js -o dist/test.js",
    "build": "browserify -d js/test.js > dist/test.js",
    "prepublish": "mkdir -p dist && npm run build"
  }
}
